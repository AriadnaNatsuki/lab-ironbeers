{{#with this}}
  <div class='card col-3'>
    <div class='card-body' style="height:30vh">
      <img
        class='card-img-top'
        alt='Responsive image'
        src='{{image_url}}'
        alt='{{name}}'
      />
      <h5 class='card-title'>{{name}}</h5>
      <p class='card-text'>{{tagline}}</p>
      <p class='card-text'><small class='text-muted'>{{description}}</small></p>
    </div>
  </div>
{{/with}}